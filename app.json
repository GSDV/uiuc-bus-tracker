{
  "expo": {
    "name": "UIUC Bus Tracker",
    "slug": "UIUCBusTracker",
    "scheme": "gsdv-uiuc-bus",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "infoPlist": {
        "CFBundleDisplayName": "UIUC Bus",
        "NSLocationWhenInUseUsageDescription": "This app needs to get your location to give you nearby stops."
      },
      "supportsTablet": false,
      "bundleIdentifier": "me.gsdv.UIUC",
      "buildNumber": "8.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
